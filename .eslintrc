{
  "rules": {
    "indent": [2, 2],
    "quotes": [2, "single"],
    "no-console": [0],
    "semi": [2, "always"]
  },
  "env": {
      "node": true,
      "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 8
  },
  "globals": {
      "process": true,
      "module": true,
      "require": true
  },
  "extends": "eslint:recommended"
}
